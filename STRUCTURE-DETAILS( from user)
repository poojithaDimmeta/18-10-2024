#include <stdio.h>
#include<string.h>
struct list
{
    char name[20];
    int age;
    int reg;
    char grade;
}detail[10];
int main()
{
    int n;
    printf("how many details");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
       printf("\n enter the name:  ");
       scanf("%s",detail[i].name);
       printf("\nenter the age:  ");
       scanf("%d",&detail[i].age);
       printf("\nenter the reg:  ");
       scanf("%d",&detail[i].reg);
       printf("\nenter the grade:  ");
       scanf(" %c",&detail[i].grade);
    }
    for(int i=0;i<n;i++)
    {
       printf("%s\n",detail[i].name);
       printf("%d\n",detail[i].age);
       printf("%d\n",detail[i].reg);
       printf("%c\n",detail[i].grade);
}
}

